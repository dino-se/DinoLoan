@model dotnet_web_mvc.Entity.Clientinfo

@{
    ViewData["Title"] = "Details";
}

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="../../client">Home</a></li>
        <li class="breadcrumb-item active">Details</li>
    </ol>
</nav>

<div class="row">

    <div class="form-group col-md-4 mt-3">
        <label>First Name</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.FirstName)" readonly>
    </div>

    <div class="form-group col-md-4 mt-3">
        <label>Middle Name</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.MiddleName)" readonly>
    </div>

    <div class="form-group col-md-4 mt-3">
        <label>Last Name</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.LastName)" readonly>
    </div>

    <div class="form-group col-md-9 mt-3">
        <label>Address</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.Address)" readonly>
    </div>

    <div class="form-group col-md-3 mt-3">
        <label>Zip Code</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.ZipCode)" readonly>
    </div>

    <div class="form-group col-md-5 mt-3">
        <label>Birthday</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.Birthday)" readonly>
    </div>

    <div class="form-group col-md-2 mt-3">
        <label>Age</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.Age)" readonly>
    </div>

    <div class="form-group col-md-6 mt-3">
        <label>Name of Father</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.NameOfFather)" readonly>
    </div>

    <div class="form-group col-md-6 mt-3">
        <label>Name of Mother</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.NameOfMother)" readonly>
    </div>

    <div class="form-group col-md-3 mt-3">
        <label>Civil Status</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.CivilStatus)" readonly>
    </div>

    <div class="form-group col-md-3 mt-3">
        <label>Religion</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.Religion)" readonly>
    </div>

    <div class="form-group col-md-3 mt-3">
        <label>Occupation</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.Occupation)" readonly>
    </div>

    <div class="form-group col-md-3 mt-3">
        <label>UserType</label>
        <input type="text" class="form-control" value="@Html.DisplayNameFor(model => model.UserType)" readonly>
    </div>
</div>